<p id="notice"><%= notice %></p>
<div class="wrapper col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
  <div class="project-text">
    <p class="left"><strong>タイトル</strong></p>
    <p class="left"><%= @project.title %></p>
    <p class="left"><strong>内容</strong></p>
    <p class="left"><%= @project.content %></p>
    <p class="left"><strong>状態</strong></p>
    <p class="left"><%= link_to @project.status, project_toggle_status_path(@project) ,method: :patch%></p>
    <p class="left"><strong>期限</strong></p>
    <p class="left"><%= @project.limit %></p>
  </div>
  <%= link_to '戻る', projects_path %>
</div>
